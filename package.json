{
  "name": "zjepe-web",
  "version": "1.0.0",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "scripts": {
    "dev": "cross-env BUILD_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "dist": " node build/build.js && node build/dist-server",
    "build": "node build/build.js",
    "build:dev": "cross-env BUILD_ENV=development npm run build",
    "build:integ": "cross-env BUILD_ENV=integrity npm run build",
    "build:report": "npm_config_report=true node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint"
